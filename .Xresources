! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #2b303b, #343d46, #2b303b

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #2b303b, #c0c5ce, #2b303b, #343d46, #eff1f5
rofi.color-active:  #2b303b, #8fa1b3, #2b303b, #343d46, #8fa1b3
rofi.color-urgent:  #2b303b, #bf616a, #2b303b, #343d46, #bf616a

! Set the desired separator style
rofi.separator-style: solid